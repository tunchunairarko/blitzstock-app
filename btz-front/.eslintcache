n\");\r\n  }\r\n\r\n  // const intl = useIntl();\r\n  return (\r\n    <Fragment>\r\n      <ProSidebar\r\n        image={sidebarBg}\r\n        collapsed={collapsed}\r\n        toggled={toggled}\r\n        breakPoint=\"md\"\r\n        onToggle={handleToggleSidebar}\r\n      >\r\n        <SidebarHeader>\r\n          <div\r\n            style={{\r\n              padding: '24px',\r\n              textTransform: 'uppercase',\r\n              fontWeight: 'bold',\r\n              fontSize: 14,\r\n              letterSpacing: '1px',\r\n              overflow: 'hidden',\r\n              textOverflow: 'ellipsis',\r\n              whiteSpace: 'nowrap',\r\n            }}\r\n          >\r\n            Main Menu\r\n        </div>\r\n        </SidebarHeader>\r\n\r\n        <SidebarContent>\r\n          <Menu iconShape=\"circle\">\r\n\r\n            <MenuItem\r\n\r\n              icon={<FaTachometerAlt />}\r\n              // suffix={<span className=\"badge red\">Dashboard</span>}\r\n            >\r\n              <Link className=\"nav-link\" to={`/`}>\r\n                Dashboard\r\n              </Link>\r\n            </MenuItem>\r\n\r\n\r\n            <MenuItem\r\n              icon={<FaGem />}>\r\n              <Link className=\"nav-link\" to={`/posting`}>\r\n                Posting Module\r\n              </Link>\r\n            </MenuItem>\r\n\r\n\r\n          </Menu>\r\n\r\n        </SidebarContent>\r\n\r\n        <SidebarFooter style={{ textAlign: 'center' }}>\r\n          <div\r\n            className=\"sidebar-btn-wrapper\"\r\n            style={{\r\n              padding: '20px 24px',\r\n            }}\r\n          >\r\n            <a\r\n              href=\"#\"\r\n              \r\n              className=\"sidebar-btn\"\r\n              rel=\"noopener noreferrer\"\r\n              onClick={logout}\r\n            >\r\n              <FaSignOutAlt />\r\n              <span> Logout</span>\r\n            </a>\r\n          </div>\r\n          \r\n        </SidebarFooter>\r\n      </ProSidebar>\r\n    </Fragment>\r\n  );\r\n};\r\n\r\nexport default Aside;","C:\\Users\\tunch\\OneDrive\\ArkoCommerce\\blitzstock-app\\mern-btz-front\\src\\components\\layout\\PostingModule\\DownloadedProductData.js",["86"],"import React, { Fragment,useState,useContext } from 'react'\r\nimport {  Form,  Row, Col, Button,  Card, ButtonGroup, Dropdown } from 'react-bootstrap'\r\nimport { FaBroom, FaDumpsterFire, FaUpload } from 'react-icons/fa';\r\nimport { CKEditor } from '@ckeditor/ckeditor5-react';\r\nimport ClassicEditor from '@ckeditor/ckeditor5-build-classic';\r\nimport \"../../../components/assets/style.css\";\r\nimport Dummy from '../../assets/dummy-prod.png';\r\nimport Axios from \"axios\";\r\nimport UserContext from \"../../../context/UserContext\";\r\nimport ErrorNotice from \"../../misc/ErrorNotice\";\r\nimport SuccessNotice from \"../../misc/SuccessNotice\";\r\n\r\n\r\n\r\n\r\nexport default function DownloadedProductData({title,upc,description,retail,image,setTitle,setRetail,setUpc,setDescription,setImage}) {\r\n    const { userData } = useContext(UserContext);\r\n    const [error, setError] = useState();\r\n    const [successNotice, setSuccessNotice] = useState();\r\n    const [quantity, setQuantity]=useState(1);\r\n    const [discount, setDiscount]=useState(50);\r\n    const [condition,setCondition]=useState('New');\r\n\r\n    const handleSelect=(e)=>{\r\n        console.log(e);\r\n        setCondition(e)\r\n      }\r\n    const increaseQuantity =() =>{\r\n        setQuantity(quantity+1);\r\n    }\r\n    const decreaseQuantity =() =>{\r\n        if(quantity!==1){\r\n            setQuantity(quantity-1);\r\n        }\r\n    }\r\n\r\n    const clearAll = () =>{\r\n        setTitle(\"\")\r\n        setUpc(\"\")\r\n        setRetail(\"\")\r\n        setDescription(\"\")\r\n        setImage(\"https://cdn.shopify.com/s/files/1/0514/3520/8854/files/surplus-auction.png?v=1609197903\")\r\n        setDiscount(\"50\")\r\n        setQuantity(\"1\")\r\n        setCondition(\"New\")\r\n\r\n    }\r\n    const postProduct = async(e) =>{\r\n        e.preventDefault()\r\n        try{\r\n            if(title===\"\" || retail===\"\" || quantity===\"\" || image===\"\" || discount===\"\"){\r\n                setError(\"Please fill up all the missing fields\")\r\n            }\r\n            else{\r\n                const res = await Axios.get(\"http://localhost:5000/products/getsku\",{headers:{\"x-auth-token\":userData.token}});\r\n                var discounted_price=parseFloat(retail)*parseInt(discount)/100;\r\n                // console.log(res.data)\r\n                if(res.data){\r\n                    var sku=res.data;\r\n                    const product={\r\n                        title:title,\r\n                        upc:upc,\r\n                        sku:sku,\r\n                        description:description,\r\n                        image:image,\r\n                        retail:retail.toString(),\r\n                        condition:condition,\r\n                        discounted_price:discounted_price.toString(),\r\n                        quantity:quantity,\r\n                        categories:[]\r\n                    }\r\n                    console.log(product)\r\n                    const username=userData.user.username;\r\n                    const productInp = product;\r\n                    const data={  username,  productInp }    \r\n                    console.log(data)\r\n                    const resp = await Axios.post(\"http://localhost:5000/products/new\",data,{headers:{\"x-auth-token\":userData.token}});\r\n                    const result=resp.data\r\n                    try{\r\n                        setSuccessNotice(\"Product uploaded successfully. Posting id: \"+result.product.id+\". SKU: \"+sku);\r\n                        clearAll();\r\n                    }catch(err){\r\n                        setError(\"Error in posting product\")\r\n                    }\r\n                }\r\n\r\n            }\r\n        }\r\n        catch(err){\r\n            console.log(err)\r\n            // err.response.data.msg && setError(err.response.data.msg);\r\n        }\r\n    }\r\n\r\n\r\n    return (\r\n        <Fragment>\r\n            {error && (\r\n                        <ErrorNotice message={error} clearError={() => setError(undefined)} />\r\n                    )}\r\n            {successNotice && (\r\n                        <SuccessNotice message={successNotice} clearSuccessNotice={() => setSuccessNotice(undefined)} />\r\n                    )}\r\n            <Card className=\"box-design mt-3\">\r\n                <Row className=\"ml-3 pr-3 mt-3\">\r\n                    <Col xs={12} sm={4} >\r\n                        <Card className=\"productImageBox\">\r\n                            <Card.Img variant=\"top\" src={image} />\r\n                            <Card.Body>\r\n                                {/* <Card.Title></Card.Title>\r\n                                <Card.Text></Card.Text> */}\r\n                                <Button variant=\"primary\" block><FaUpload /> Upload manually</Button>\r\n                                <Button variant=\"danger\" block><FaDumpsterFire /> Clear photo</Button>\r\n                            </Card.Body>\r\n                        </Card>\r\n                    </Col>\r\n                    <Col xs={12} sm={8}>\r\n                        <Form>\r\n                            <Form.Group as={Row} controlId=\"titleText\">\r\n                                <Form.Label column sm=\"2\">\r\n                                    Title\r\n                                </Form.Label>\r\n                                <Col sm=\"10\">\r\n                                    <Form.Control placeholder=\"Enter Title\" value={title} onChange={(e)=>setTitle(e.target.value)}/>\r\n                                </Col>\r\n                            </Form.Group>\r\n\r\n                            <Form.Group as={Row} controlId=\"upcText\">\r\n                                <Form.Label column sm=\"2\">\r\n                                    UPC/ASIN\r\n                                </Form.Label>\r\n                                <Col sm=\"10\">\r\n                                    <Form.Control placeholder=\"Enter UPC/Asin\" value={upc} onChange={(e)=>setUpc(e.target.value)}/>\r\n                                </Col>\r\n                            </Form.Group>\r\n\r\n                            <Form.Group as={Row} controlId=\"priceText\">\r\n                                <Form.Label column sm=\"2\">\r\n                                    Retail\r\n                                </Form.Label>\r\n                                <Col sm=\"10\">\r\n                                    <Form.Control placeholder=\"Enter Retail\" value={retail} onChange={(e)=>setRetail(e.target.value)}/>\r\n                                </Col>\r\n                            </Form.Group>\r\n                            <Form.Group as={Row} controlId=\"discountText\">\r\n                                <Form.Label column sm=\"2\">\r\n                                    Discount\r\n                                </Form.Label>\r\n                                <Col sm=\"3\">\r\n                                    <Form.Control placeholder=\"Enter Discount\" value={discount} onChange={(e)=>setDiscount(e.target.value)}/>\r\n                                </Col>\r\n                                <Form.Label column sm=\"2\">\r\n                                    Qty\r\n                                </Form.Label>\r\n                                <Col sm=\"3\">\r\n                                    <Form.Control placeholder=\"Enter Qty\" value={quantity} onChange={(e)=>setDiscount(e.target.value)}/>\r\n                                </Col>\r\n                                <Col sm=\"2\">\r\n                                    <ButtonGroup id=\"qtyButtonGroup\" aria-label=\"Basic example\">\r\n                                        <Button variant=\"outline-danger\" className=\"qtyButton\" onClick={increaseQuantity}>+</Button>\r\n                                        <Button variant=\"outline-danger\" className=\"qtyButton\" onClick={decreaseQuantity}>-</Button>\r\n                                    </ButtonGroup>\r\n                                </Col>\r\n                            </Form.Group>\r\n                            <Form.Group as={Row} controlId=\"formPlaintextEmail\">\r\n                                <Form.Label column sm=\"2\">\r\n                                    Condition\r\n                                </Form.Label>\r\n                                <Col sm=\"4\">\r\n                                    <Dropdown onSelect={handleSelect}>\r\n                                        <Dropdown.Toggle variant=\"outline-secondary\" id=\"dropdown-basic\" block >\r\n                                            {condition}\r\n                                        </Dropdown.Toggle >\r\n\r\n                                        <Dropdown.Menu >\r\n                                            <Dropdown.Item eventKey=\"New\">New</Dropdown.Item>\r\n                                            <Dropdown.Item eventKey=\"Like New\">Like New</Dropdown.Item>\r\n                                            <Dropdown.Item eventKey=\"Opened Box\">Opened Box</Dropdown.Item>\r\n                                            <Dropdown.Item eventKey=\"Damaged Box\">Damaged Box</Dropdown.Item>\r\n                                            <Dropdown.Item eventKey=\"Uninspected Returns\">Uninspected Returns</Dropdown.Item>\r\n                                            <Dropdown.Item eventKey=\"Salvage\">Salvage</Dropdown.Item>\r\n                                        </Dropdown.Menu>\r\n                                    </Dropdown>\r\n                                </Col>\r\n                                <Form.Label column sm=\"2\">\r\n                                    Categories\r\n                                </Form.Label>\r\n                                <Col sm=\"4\">\r\n                                    <Form.Control placeholder=\"Separate by comma (,)\" disabled/>\r\n                                </Col>\r\n                            </Form.Group>\r\n                            <Form.Group as={Row} controlId=\"formPlaintextEmail\">\r\n                                <Form.Label column sm=\"2\">\r\n                                    Description\r\n                                </Form.Label>\r\n                                <Col sm=\"10\">\r\n                                    <CKEditor\r\n                                        editor={ClassicEditor}\r\n                                        data= {description}\r\n                                        config={{\r\n                                            removePlugins: ['Image', 'ImageCaption', 'ImageStyle', 'ImageToolbar', 'ImageUpload', 'MediaEmbed']\r\n                                        }}\r\n                                        // onReady={editor => {\r\n                                        //     // You can store the \"editor\" and use when it is needed.\r\n                                        //     console.log('Editor is ready to use!', editor);\r\n                                        // }}\r\n                                        onChange={(event, editor) => {\r\n                                            const data = editor.getData();\r\n                                            setDescription(data);\r\n                                            // console.log({ event, editor, data });\r\n                                        }}\r\n                                        // onBlur={(event, editor) => {\r\n                                        //     console.log('Blur.', editor);\r\n                                        // }}\r\n                                        // onFocus={(event, editor) => {\r\n                                        //     console.log('Focus.', editor);\r\n                                        // }}\r\n                                    />\r\n                                </Col>\r\n                            </Form.Group>\r\n                            <Form.Group as={Row} controlId=\"formPlaintextEmail\">\r\n                                <Col sm=\"6\" className=\"mt-2\">\r\n                                    <Button variant=\"success\" block onClick={postProduct}><FaUpload /> Post Product </Button>\r\n                                </Col>\r\n                                <Col sm=\"6\" className=\"mt-2\">\r\n                                    <Button variant=\"danger\" block onClick={clearAll}><FaBroom /> Clear All </Button>\r\n                                </Col>\r\n                            </Form.Group>\r\n                        </Form>\r\n                    </Col>\r\n                </Row>\r\n            </Card>\r\n                                            \r\n        </Fragment>\r\n    )\r\n}\r\n","C:\\Users\\tunch\\OneDrive\\ArkoCommerce\\blitzstock-app\\mern-btz-front\\src\\components\\layout\\PostingModule\\SearchModule.js",[],"C:\\Users\\tunch\\OneDrive\\ArkoCommerce\\blitzstock-app\\mern-btz-front\\src\\components\\layout\\PostingModule\\SearchModal.js",["87"],"import React, { Fragment, useEffect, useState } from 'react'\r\nimport { Modal, Button, Table, Image } from 'react-bootstrap'\r\nimport Axios from \"axios\";\r\n\r\nconst SearchModal = ({show,handleClose,searchQuery,onProductChosen,marketplace}) => {\r\n\r\n    const [userChosenProduct, setUserChosenProduct] = useState({});\r\n    \r\n    //popup theke data niye ekhon form e boshaite hobe\r\n    const [currentProductData, setCurrentProductData] = useState({\r\n        productList:undefined,\r\n    });\r\n    useEffect(() =>{ \r\n        const getProductList = async () => {\r\n            let token = localStorage.getItem(\"auth-token\");\r\n            if(token==null){\r\n                localStorage.setItem(\"auth-token\",\"\");\r\n                token=\"\";\r\n            }\r\n            else{\r\n                const tokenResponse = await Axios.post(\r\n                    \"http://localhost:5000/users/tokenIsValid\",\r\n                    null,\r\n                    {headers:{\"x-auth-token\":token}}\r\n                );\r\n                // console.log(searchQuery)\r\n                const body ={ searchQuery, marketplace }\r\n                if(tokenResponse.data){\r\n                    const productRes = await Axios.post(\r\n                        \"http://localhost:5000/products\",body,{headers:{\"x-auth-token\":token}},\r\n                    )\r\n                    \r\n                    setCurrentProductData({\r\n                        productList: productRes.data,\r\n                    });\r\n                }\r\n                \r\n            }\r\n        }\r\n        getProductList();\r\n    },[show])\r\n\r\n    const chooseProduct = async(e) =>{\r\n        e.preventDefault();\r\n        if(userChosenProduct){\r\n            onProductChosen(userChosenProduct)\r\n            handleClose();\r\n        }\r\n    }\r\n\r\n    \r\n\r\n    return (\r\n        <Fragment>\r\n            <Modal size=\"lg\" show={show} onHide={handleClose} backdrop=\"static\" keyboard={false} centered>\r\n                <Modal.Header closeButton>\r\n                    <Modal.Title id=\"searchResTitle\">Search result for {searchQuery}</Modal.Title>\r\n                </Modal.Header>\r\n                <Modal.Body>\r\n                    {currentProductData.productList ?(\r\n                        <Table className=\"searchModalTable\" striped bordered hover size=\"sm\" responsive>\r\n                            <thead>\r\n                                <tr>\r\n                                    <th>#</th>\r\n                                    <th>Image</th>\r\n                                    <th>UPC/